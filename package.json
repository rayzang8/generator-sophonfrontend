{
  "name": "generator-sophonfrontend",
  "version": "1.0.3",
  "description": "Transwarp Sophon Frontend Project Scaffolder",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --open --config ./config/webpack/webpack.dev.babel.js",
    "build": "webpack --config ./config/webpack/webpack.prod.babel.js",
    "build:analysis": "webpack --config ./config/webpack/webpack.prod.babel.js --env=analysis",
    "postbuild": "yarn copy-build-resource",
    "copy-build-resource": "node ./config/scripts/copy-build-resource.js",
    "autofix": "npx eslint --fix ./src/**/*{.ts,.tsx} && npx stylelint --config \"./stylelint.config.js\"  --customSyntax postcss-scss \"./src/**/*.scss\" --fix && npx stylelint --config \"./stylelint.config.js\"  --customSyntax postcss-less \"./src/**/*.less\" --fix",
    "cleanup-templates": "rm -rf ./generators/app/templates",
    "generate-dirs": "mkdir -p generators/app/templates/rootConfigs && mkdir -p generators/app/templates/sophon/src && mkdir -p generators/app/templates/sophon/config",
    "copy-resources": "cp -a ./src/. generators/app/templates/sophon/src &&  cp -a ./config/. generators/app/templates/sophon/config && cp -a ./rootConfigs/. generators/app/templates/rootConfigs && cp tsconfig.json babel.config.json postcss.config.js .eslintrc.js stylelint.config.js generators/app/templates/rootConfigs/",
    "generate-template": "yarn run cleanup-templates && yarn run generate-dirs && yarn run copy-resources"
  },
  "author": "Sophon",
  "license": "UNLICENSED",
  "files": [
    "generators"
  ],
  "repository": {},
  "keywords": [
    "yeoman-generator",
    "yeoman",
    "generator",
    "react",
    "scaffold",
    "scaffolder",
    "template",
    "typescript",
    "frontend",
    "webpack",
    "sophon"
  ],
  "dependencies": {
    "@popperjs/core": "^2.9.3",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/react-router": "^5.1.16",
    "@types/react-router-dom": "^5.1.8",
    "font-awesome": "^4.7.0",
    "history": "^5.0.1",
    "mem-fs": "^2.1.0",
    "mem-fs-editor": "^9.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "6.0.0-beta.2",
    "react-router-dom": "6.0.0-beta.2",
    "yeoman-assert": "3.1.1",
    "yeoman-environment": "3.6.0",
    "yeoman-generator": "5.4.2",
    "yeoman-test": "6.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/register": "^7.15.3",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "autoprefixer": "^10.3.4",
    "babel-loader": "^8.2.2",
    "chalk": "4.1.2",
    "core-js": "3",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "eslint": "^7.32.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "html-webpack-plugin": "^5.3.2",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lodash": "4.17.21",
    "mini-css-extract-plugin": "^2.4.5",
    "postcss": "^8.3.6",
    "postcss-less": "^5.0.0",
    "postcss-loader": "^6.1.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-scss": "^4.0.2",
    "progress-bar-webpack-plugin": "^2.1.0",
    "recursive-copy": "^2.0.13",
    "sass": "1.32",
    "sass-loader": "^12.1.0",
    "source-map-loader": "^3.0.0",
    "speed-measure-webpack-plugin": "^1.5.0",
    "style-loader": "^3.2.1",
    "stylelint": "^14.1.0",
    "stylelint-config-recommended-less": "^1.0.1",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-less": "^1.0.1",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.0.0",
    "stylelint-webpack-plugin": "^3.1.0",
    "terser-webpack-plugin": "^5.2.5",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
